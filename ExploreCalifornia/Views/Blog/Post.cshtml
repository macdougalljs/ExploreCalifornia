@model ExploreCalifornia.Models.Post
@{
    Layout = "_Layout";
}

<article class="blog-post">
    <h1>@Model.Title</h1>
    <div class="details">
        Posted <span>@Model.Posted</span> by <span>@Model.Author</span>
    </div>
    <div class="content">
        @Model.Body
    </div>
</article>
